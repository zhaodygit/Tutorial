@model IList<string>

<form method="post">

    @for (var i = 0; i < 10; i++)
    {
        <div class="form-group row">
            <label class="col-sm-2">Item: @(i + 1)</label>
            <div class="col-sm-10">
                <input class="form-control" asp-for="@Model[i]" />
            </div>
        </div>
    }

    <button type="submit" class="btn btn-primary">提交</button>

</form>   